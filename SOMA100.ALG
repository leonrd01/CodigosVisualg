algoritmo "Soma100"
var
    cont,tot:inteiro
inicio
        para cont <- 1 ate 100 passo 1 faca
            tot <- tot + cont
            escreva(cont)


        fimpara
            escreval(" ")
            escreva("A soma dos 100 primeiro números inteiros é de: ",tot)
            
            
            
fimalgoritmo