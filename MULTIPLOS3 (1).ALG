algoritmo "Multiplos3"
var
      nmr,cont,tot:inteiro
inicio
      escreva("insira um número: ")
         leia(nmr)

        repita
            escreva(cont,"...")
             tot <- cont + tot
              cont <- cont + 3

         
        ate(cont > nmr)
           escreval(" ")
           escreva("a soma dos números é de: ",tot)









fimalgoritmo